{
  "kind": "collectionType",
  "collectionName": "form_submissions",
  "info": {
    "singularName": "form-submission",
    "pluralName": "form-submissions",
    "displayName": "Form Submission",
    "description": "Submitted form data"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "form": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::form.form",
      "inversedBy": "submissions"
    },
    "formName": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "json",
      "required": true
    },
    "submittedAt": {
      "type": "datetime",
      "required": true
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["files"]
    },
    "pdf": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["files"],
      "private": true,
      "plugin": "upload",
      "visible": false

    },
    "ip": {
      "type": "string"
    },
    "userAgent": {
      "type": "string"
    },
    "locale": {
      "type": "string"
    }
  }
}

